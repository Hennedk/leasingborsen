# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Railway PDF Extraction Service
VITE_PDF_SERVICE_URL=https://leasingborsen-production.up.railway.app

# AI Provider Configuration
# Primary AI Provider (OpenAI)
VITE_OPENAI_API_KEY=sk-your-openai-api-key
VITE_OPENAI_MODEL=gpt-4-turbo-preview        # Model to use (default: gpt-4-turbo-preview)

# Fallback AI Provider (Anthropic)
VITE_ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
VITE_ANTHROPIC_MODEL=claude-3-opus-20240229  # Model to use (default: claude-3-opus)

# Feature Flags
VITE_AI_EXTRACTION_ENABLED=false              # Enable AI extraction (default: false)
VITE_AI_PROVIDER_PRIMARY=openai               # Primary provider: openai|anthropic (default: openai)
VITE_AI_PROVIDER_FALLBACK=anthropic           # Fallback provider: openai|anthropic|none (default: anthropic)

# Cost Controls
VITE_MAX_TOKENS_PER_PDF=8000                  # Maximum tokens per PDF (default: 8000)
VITE_MAX_COST_PER_PDF_CENTS=20                # Maximum cost per PDF in cents (default: 20)
VITE_DAILY_COST_LIMIT_USD=10                  # Daily cost limit in USD (default: 10)

# Extraction Settings
VITE_EXTRACTION_TIMEOUT_SECONDS=60            # Timeout for AI extraction (default: 60)
VITE_EXTRACTION_MAX_RETRIES=2                 # Maximum retry attempts (default: 2)
VITE_EXTRACTION_CONFIDENCE_THRESHOLD=0.8      # Minimum confidence score (default: 0.8)

# Monitoring & Alerts
VITE_EXTRACTION_LOG_LEVEL=info                # Log level: debug|info|warn|error (default: info)
VITE_ALERT_EMAIL=admin@leasingborsen.dk       # Email for cost alerts
VITE_ALERT_COST_THRESHOLD_USD=50              # Send alert when daily cost exceeds this (default: 50)

# Example Development Setup:
# 1. Copy this file to .env.local
# 2. Add your Supabase credentials
# 3. Optionally add OpenAI API key for AI extraction
# 4. System will work with pattern-only extraction if no AI key provided
{
  "name": "leasingborsen-react-production",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:refactored": "vitest run src/hooks/useBatchReviewState.simple.test.ts src/components/admin/batch/BatchReviewHeader.simple.test.tsx",
    "build:test": "npm run test:refactored && npm run build",
    "env:check": "npx tsx scripts/verify-environment.ts",
    "env:verify": "npm run env:check",
    "pdf:test": "node scripts/archive/test-pdf-processing.js",
    "pdf:test-toyota": "node scripts/archive/test-toyota-pdf.js",
    "pdf:load-configs": "node scripts/archive/load-dealer-configs.js",
    "pdf:deploy": "supabase functions deploy process-pdf",
    "staging:setup": "./scripts/setup-staging.sh",
    "staging:test": "NODE_ENV=staging npx tsx scripts/verify-environment.ts",
    "staging:deploy-functions": "supabase functions deploy --project-ref $STAGING_PROJECT_REF",
    "staging:deploy-all": "npm run staging:deploy-functions && echo 'âœ… All Edge Functions deployed to staging'",
    "staging:seed": "npx tsx scripts/seed-staging-data.ts",
    "staging:db-push": "supabase db push",
    "staging:dev": "NODE_ENV=staging npm run dev",
    "staging:monitor": "echo 'Opening staging monitoring dashboard...'",
    "staging:logs": "echo 'Viewing staging logs in deploy/staging/logs/'",
    "staging:backup": "echo 'Creating staging backup...'",
    "staging:rollback": "echo 'Rolling back staging deployment...'",
    "preview:url": "npx tsx scripts/preview-helper.ts url",
    "preview:deploy": "npx tsx scripts/preview-helper.ts deploy",
    "preview:branch": "npx tsx scripts/preview-helper.ts branch",
    "preview:reset-data": "npx tsx scripts/preview-helper.ts reset-data",
    "preview:help": "npx tsx scripts/preview-helper.ts",
    "prompts:test": "npx tsx scripts/prompt-manager/quick-test.ts",
    "prompts:create": "npx tsx scripts/manage-prompts.ts create",
    "prompts:list": "npx tsx scripts/manage-prompts.ts list",
    "prompts:list-openai": "npx tsx scripts/manage-prompts.ts list-openai",
    "prompts:get": "npx tsx scripts/manage-prompts.ts get",
    "prompts:sync": "npx tsx scripts/manage-prompts.ts sync",
    "prompts:update-id": "npx tsx scripts/manage-prompts.ts update-id",
    "prompts:example": "npx tsx scripts/prompt-manager/example-usage.ts",
    "prompts:test:unit": "vitest run scripts/prompt-manager/__tests__/prompt-manager.test.ts",
    "prompts:test:integration": "INTEGRATION_TEST=true vitest run scripts/prompt-manager/__tests__/integration.test.ts",
    "test:extraction": "vitest --config vitest.config.extraction.ts",
    "test:extraction:watch": "vitest --config vitest.config.extraction.ts --watch",
    "test:extraction:coverage": "vitest run --config vitest.config.extraction.ts --coverage",
    "test:extraction:ui": "vitest --config vitest.config.extraction.ts --ui",
    "test:edge": "cd supabase/functions && deno test --allow-all",
    "test:edge:apply-changes": "cd supabase/functions/apply-extraction-changes && deno test --allow-all __tests__/",
    "test:edge:compare": "cd supabase/functions/compare-extracted-listings && deno test --allow-all __tests__/",
    "test:edge:coverage": "cd supabase/functions && deno test --allow-all --coverage=cov_profile && deno coverage cov_profile",
    "test:all": "npm run test && npm run test:edge",
    "test:comparison": "vitest run src/services/comparison/__tests__/",
    "test:comparison:watch": "vitest watch src/services/comparison/__tests__/"
  },

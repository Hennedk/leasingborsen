{
  "patterns": {
    "aygo_x": {
      "manual_detection": "(?<!automatgear)(?:\\s|$)",
      "automatic_detection": "automatgear",
      "variant_patterns": [
        "(Active|Pulse)(?:\\s+X-Clusiv)?",
        "(Active|Pulse)\\s+X-Clusiv"
      ],
      "expected_variants": [
        "Active",
        "Active X-Clusiv", 
        "Pulse",
        "Pulse X-Clusiv"
      ]
    },
    "bz4x": {
      "awd_detection": "AWD",
      "power_patterns": [
        "(\\d+\\.?\\d*)\\s*kWh,?\\s*(\\d+)\\s*hk(?:\\s+(AWD))?",
        "(\\d+\\.?\\d*)\\s*Kwh,?\\s*(\\d+)\\s*Hk(?:\\s+(AWD))?",
        "(\\d+[,.]\\d*)\\s*[Kk][Ww][Hh].*?(\\d+)\\s*[Hh][Kk].*?(AWD)?"
      ],
      "expected_variants": [
        "Active",
        "Executive", 
        "Executive Panorama"
      ],
      "power_levels": {
        "167": "FWD",
        "224": "FWD", 
        "343": "AWD"
      }
    },
    "yaris_cross": {
      "high_power_variants": ["Elegant", "GR Sport"],
      "standard_power_variants": ["Active", "Active Safety", "Style Comfort", "Style Safety"],
      "power_patterns": [
        "1\\.5\\s+Hybrid\\s+(\\d+)\\s+hk(?:\\s+automatgear)?",
        "1\\.8\\s+Hybrid\\s+(\\d+)\\s+hk(?:\\s+automatgear)?",
        "(\\d+)\\s*(?:hk|hp)"
      ],
      "power_thresholds": {
        "standard": 116,
        "high": 130,
        "elegant": 140
      }
    },
    "yaris": {
      "variant_patterns": [
        "(Active|Style|Style Comfort|Style Technology)",
        "Acitve"
      ],
      "variant_corrections": {
        "Acitve": "Active"
      }
    },
    "corolla": {
      "variant_patterns": [
        "(Active Comfort|Active Comfort Plus|Style|Style Safety)"
      ]
    },
    "urban_cruiser": {
      "variant_patterns": [
        "(Active|Executive|Executive Comfort)"
      ]
    },
    "transmission": {
      "automatic_indicators": [
        "automatgear",
        "aut\\.",
        "automatic",
        "CVT"
      ],
      "manual_indicators": [
        "manual",
        "gear",
        "MT"
      ]
    },
    "drivetrain": {
      "awd_indicators": [
        "AWD",
        "4WD", 
        "All-wheel drive",
        "4x4"
      ],
      "fwd_indicators": [
        "FWD",
        "Front-wheel drive",
        "2WD"
      ]
    },
    "engine": {
      "power_extraction": [
        "(\\d+)\\s*(?:hk|hp|HK|HP)",
        "(\\d+)\\s*(?:Hk|Hp)",
        "(?:Power:?\\s*)?(\\d+)\\s*(?:hk|hp)"
      ],
      "battery_extraction": [
        "(\\d+[.,]?\\d*)\\s*kWh",
        "(\\d+[.,]?\\d*)\\s*KWh",
        "(\\d+[.,]?\\d*)\\s*Kwh"
      ],
      "displacement_extraction": [
        "(\\d+\\.\\d+)\\s*(?:L|l|liter|litre)",
        "(\\d+\\.\\d+)\\s*benzin",
        "(\\d+\\.\\d+)\\s*Hybrid"
      ]
    }
  },
  "thresholds": {
    "confidence_minimum": 0.7,
    "power_hp_minimum": 50,
    "power_hp_maximum": 500,
    "battery_kwh_minimum": 10.0,
    "battery_kwh_maximum": 150.0,
    "displacement_minimum": 0.8,
    "displacement_maximum": 4.0
  },
  "expected_totals": {
    "aygo_x": 4,
    "yaris": 4,
    "yaris_cross": 6,
    "corolla": 4,
    "bz4x": 6,
    "urban_cruiser": 3,
    "total": 27
  },
  "extraction_methods": {
    "aygo_x_transmission": {
      "priority": ["raw_line_context", "engine_specification", "variant_name"],
      "fallback": "manual"
    },
    "bz4x_drivetrain": {
      "priority": ["engine_specification", "power_level", "variant_name"],
      "fallback": "fwd"
    },
    "yaris_cross_power": {
      "priority": ["engine_specification", "variant_name"],
      "fallback": "standard"
    }
  },
  "validation_rules": {
    "required_fields": ["model", "variant", "engine_specification"],
    "unique_signature_fields": ["model", "variant", "engine_specification", "transmission_type", "drivetrain_type"],
    "model_specific_validations": {
      "AYGO X": {
        "must_have_transmission": true,
        "expected_transmissions": ["manual", "automatic"]
      },
      "BZ4X": {
        "must_have_drivetrain": true,
        "expected_drivetrains": ["fwd", "awd"]
      },
      "YARIS CROSS": {
        "must_have_power": true,
        "power_variants": ["116", "130", "140"]
      }
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "enable_statistics": true,
    "enable_pattern_tracking": true
  }
}